{
    "rules": {
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "no-var-keyword": true,
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "no-internal-module": true,
        "no-trailing-whitespace": true,        
        "prefer-const": false,
        "jsdoc-format": true
    },
    "jsRules": {},        
    "rulesDirectory": []    
}