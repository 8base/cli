{
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "no-var-keyword": true,
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "no-internal-module": true,
    "no-trailing-whitespace": true,
    "no-floating-promises": true,
    "prefer-const": false,
    "jsdoc-format": true,
    "quotemark": [
      true,
      "double"
    ]
  },
  "jsRules": {}
}
