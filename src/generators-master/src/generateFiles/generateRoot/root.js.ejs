import React from 'react';
import { Redirect, Route, Switch } from 'react-router-dom';

import { ProtectedRoute } from './shared/components';
import { MainPlate, ContentPlate, Nav } from './components';
import { Auth } from './routes/auth';
<% screens.forEach(({ screenName, routeUrl }) => { 
  %><%- chunks.routeImport({ screenName: screenName, changeCase: changeCase })%><% 
}); %>


export const Root = () => (
  <Switch>
    <Route path="/auth" component={Auth} />
    <ProtectedRoute>
      <MainPlate>
        <Nav.Plate color="BLUE">
        <% screens.forEach(({ screenName, routeUrl }) => { 
          %><%- chunks.routeLink({ routeUrl: routeUrl, screenName: screenName, changeCase: changeCase })%><% 
        }); %>
        </Nav.Plate>
        <ContentPlate>
          <Switch>
        <% screens.forEach(({ screenName, routeUrl }) => { 
          %><%- chunks.routeComponent({ routeUrl: routeUrl, screenName: screenName, changeCase: changeCase })%><% 
        }); %>
          </Switch>
        </ContentPlate>
      </MainPlate>
    </ProtectedRoute>
  </Switch>
);
