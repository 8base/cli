{
  "name": "8base-cli-continued",
  "version": "1.0.0-beta.10",
  "description": "8base CLI",
  "author": "8base",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "8base": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/trakservices/8base-cli-continued.git"
  },
  "files": [
    "dist",
    "templates",
    "postinstall.js"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "npm run build -- --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext js,ts src",
    "lint:fix": "npm run lint --fix",
    "prettier:check": "prettier --list-different \"{src,test}/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.{ts,tsx}\"",
    "e2e": "vitest run --config e2e/vitest.config.ts",
    "e2e:watch": "vitest --config e2e/vitest.config.ts",
    "e2e:update-snapshot": "vitest run --config e2e/vitest.config.ts --update",
    "upload": "npm version patch && git push --follow-tags",
    "postinstall": "node ./scripts/is-ci.cjs || (node ./scripts/postinstall.js && (test -e dist/index.js && node dist/index.js --help || echo))"
  },
  "dependencies": {
    "@8base/api-client": "^3.0.0-beta.3",
    "@8base/error-codes": "^1.0.23",
    "@paralleldrive/cuid2": "^2.2.2",
    "adm-zip": "^0.5.10",
    "archiver": "^5.3.1",
    "chalk": "^4.1.2",
    "ci-info": "^3.8.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fs-extra": "^11.1.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^23.2.8",
    "ignore": "^5.2.4",
    "isomorphic-fetch": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "memorystream": "^0.3.1",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "simple-git": "^3.20.0",
    "table": "^6.8.1",
    "tree-node-cli": "^1.6.0",
    "typescript": "^4.3.5",
    "validate-npm-package-name": "^5.0.0",
    "winston": "^3.10.0",
    "yaml": "^2.3.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.2",
    "@types/archiver": "^5.3.3",
    "@types/ejs": "^3.1.3",
    "@types/fs-extra": "^11.0.2",
    "@types/isomorphic-fetch": "^0.0.37",
    "@types/lodash": "^4.14.199",
    "@types/memorystream": "^0.3.2",
    "@types/node": "^18.16.18",
    "@types/prompts": "^2.4.5",
    "@types/ramda": "^0.26.44",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/yargs": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vitest": "^0.3.1",
    "execa": "^8.0.1",
    "prettier": "^3.0.3",
    "strip-ansi": "^7.1.0",
    "vitest": "^0.34.5"
  }
}
