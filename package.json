{
  "name": "8base-cli",
  "version": "0.1.3-rc20",
  "description": "8base CLI",
  "author": "8base",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "8base": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/8base/cli.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "npm run build -- --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint --ext js,ts src",
    "lint:fix": "npm run lint --fix",
    "prettier:check": "prettier --list-different \"{src,test}/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.{ts,tsx}\"",
    "e2e": "jest --config e2e/config.json",
    "scaffold:generate": "node ./e2e-scaffold/scripts/generateScaffold.js",
    "scaffold:start": "npm run start --prefix=./temp/app-example",
    "scaffold:prepare-e2e": "npm run scaffold:generate && npm run scaffold:start",
    "e2e-scaffold": "jest --config ./e2e-scaffold/config.json",
    "e2e-scaffold:debug": "cross-env E2E_SCAFFOLD_DEBUG=true npm run e2e-scaffold",
    "e2e-scaffold:ci": "cross-env E2E_SCAFFOLD_GENERATE=true npm run e2e-scaffold",
    "upload": "npm version patch && git push --follow-tags",
    "postinstall": "node ./postinstall.js && (test -e dist/index.js && node dist/index.js --help || echo)"
  },
  "dependencies": {
    "@8base/api-client": "^0.33.1",
    "@8base/error-codes": "^1.0.23",
    "@8base/generators": "^0.33.0",
    "@8base/utils": "^0.33.1",
    "@paralleldrive/cuid2": "^2.2.1",
    "adm-zip": "^0.5.10",
    "archiver": "^5.3.1",
    "chalk": "^3.0.0",
    "dotenv": "^16.3.1",
    "download": "^8.0.0",
    "ejs": "^3.1.9",
    "fs-extra": "^11.1.1",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^23.2.3",
    "ignore": "^5.2.4",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "memorystream": "^0.3.1",
    "mkdirp": "^3.0.1",
    "open": "8.4.2",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "readdir": "^1.0.2",
    "simple-git": "^3.19.1",
    "table": "^6.8.1",
    "tree-node-cli": "^1.6.0",
    "typescript": "^4.3.5",
    "validate-npm-package-name": "^5.0.0",
    "winston": "^3.9.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/archiver": "^5.3.2",
    "@types/download": "^8.0.2",
    "@types/ejs": "^3.1.2",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/jest-image-snapshot": "^6.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.4.0",
    "@types/memorystream": "^0.3.1",
    "@types/mkdirp": "^2.0.0",
    "@types/node": "^20.3.1",
    "@types/prompts": "^2.4.4",
    "@types/ramda": "^0.26.44",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "5.1.1",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jest-environment-node-debug": "2.0.0",
    "jest-image-snapshot": "6.1.0",
    "jest-transform-graphql": "2.1.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "puppeteer": "^19.11.1",
    "strip-ansi": "^6.0.1",
    "ts-jest": "^29.1.0"
  }
}
