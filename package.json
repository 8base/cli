    "graphql-request": "^1.4.1",
    "graphql-server-lambda": "^1.3.2",
    "graphql-tools": "^2.12.0",
    "i18next": "^11.8.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.9.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "memfs": "^2.6.2",
    "memorystream": "^0.3.1",
    "minimist": "^1.2.0",
    "morgan": "^1.9.1",
