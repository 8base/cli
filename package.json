{
  "name": "8base",
  "version": "0.0.63",
  "description": "8base CLI",
  "author": "8base",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "8base": "dist/index.js"
  },
  "scripts": {
    "e2e": "npm run build && jest --config e2e/config.json",
    "test": "npm run e2e && npm run tslint",
    "build": "tsc",
    "watch": "npm run build -- --watch",
    "tslint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts'",
    "upload": "npm version patch && git push --follow-tags",
    "preversion": "npm run tslint"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "clearMocks": true,
    "setupTestFrameworkScriptFile": "./test/setup.ts"
  },
  "dependencies": {
    "@8base/api-client": "^0.14.2",
    "@8base/generators": "^0.14.2",
    "@8base/utils": "^0.14.2",
    "@types/execa": "^0.9.0",
    "archiver": "^2.1.1",
    "async": "^2.5.0",
    "chalk": "^2.4.1",
    "change-case": "^3.1.0",
    "cuid": "^2.1.4",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "graceful-fs": "^4.1.11",
    "graphql": "^0.13.0",
    "graphql-request": "^1.4.1",
    "graphql-server-lambda": "^1.3.2",
    "graphql-tools": "^2.12.0",
    "i18next": "^11.8.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "3.9.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "memfs": "^2.6.2",
    "memorystream": "^0.3.1",
    "minimist": "^1.2.0",
    "morgan": "^1.9.1",
    "opn": "^5.4.0",
    "ora": "^3.0.0",
    "path": "^0.12.7",
    "pluralize": "^7.0.0",
    "prompts": "^1.1.0",
    "readdir": "^0.0.13",
    "recursive-readdir": "^2.2.1",
    "request": "^2.88.0",
    "stream-to-buffer": "^0.1.0",
    "typescript": "^2.5.1",
    "winston": "^3.1.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@types/archiver": "^2.1.2",
    "@types/body-parser": "^1.16.8",
    "@types/chalk": "^2.2.0",
    "@types/cuid": "^1.3.0",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.0",
    "@types/graphql": "^0.12.1",
    "@types/i18next": "^8.4.5",
    "@types/jest": "^20.0.8",
    "@types/js-yaml": "^3.10.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.74",
    "@types/minimist": "^1.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.3.0",
    "@types/opn": "^5.1.0",
    "@types/ora": "^1.3.4",
    "@types/recursive-readdir": "^2.2.0",
    "@types/request": "^2.47.0",
    "@types/supertest": "^2.0.3",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.4.4",
    "@types/ws": "^3.2.0",
    "@types/yargs": "^11.1.1",
    "@types/zen-observable": "^0.5.3",
    "awesome-typescript-loader": "^3.2.3",
    "jest": "^21.0.1",
    "jest-cli": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "jest-transform-graphql": "^2.1.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.5",
    "npm": "^6.4.1",
    "ts-jest": "^21.2.2",
    "tslib": "^1.8.0",
    "tslint": "^5.13.0",
    "tslint-eslint-rules": "^5.4.0"
  }
}
