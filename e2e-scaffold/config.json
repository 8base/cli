{
  "setupFiles": [
    "./setup/setup.ts"
  ],
  "setupFilesAfterEnv": [
    "./setup/setupTestFramework.ts"
  ],
  "globalSetup": "./setup/globalSetup.ts",
  "globalTeardown": "./setup/globalTeardown.ts",
  "testEnvironment": "./setup/testEnvironment.js",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "transform": {
    "^.+\\.tsx?$": [
      "ts-jest",
      {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    ]
  },
  "testMatch": [
    "/**/*.test.(t|j)s"
  ],
  "preset": "ts-jest"
}
